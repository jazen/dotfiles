# emacs
alias e='emacs-snapshot'
alias se='sudo emacs-snapshot'

# git
alias gs='git status'
alias gd='git diff'
alias ga='git add'
alias gc='git commit -m'
alias gca='git commit -a -m'
alias gcam='git commit --amend -C HEAD'
alias gp='git push'

# ls
alias l='ls'
alias ll='ls -l'
alias lla='ls -la'

# misc
alias g='grep'
alias p='ps aux | grep'
